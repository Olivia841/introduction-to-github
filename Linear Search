#include<bits/stdc++.h>
using namespace std;

int Liner_Search(int arr[],int n, int target){

    for(int i=0;i<n;i++){
        if(arr[i]==target){
        return i;

    }


    }
    return -1;


}

int main(){
    int n;
    cout<<"Enter the size of array=";
    cin>>n;

    cout<<"Enter the elements of array= ";
    int arr[n];

    for(int i=0; i<n;i++){

        cin>>arr[i];
    }

    int target;
    cout<<"Enter the target value=";
    cin>>target;
    int result=Liner_Search(arr,n,target);

    if(result==-1){
       cout<<"Not found";

    }
    else{
        cout<<"Found at index no="<<result<<endl;
    }

    return 0;
}
